{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build && npm run build-storybook",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "./dist",
  "routes": [
    {
      "src": "/storybook/(.*)",
      "dest": "/storybook-static/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}